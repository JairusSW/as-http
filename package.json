  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JairusSW/as-http/issues"
  },
  "homepage": "https://github.com/JairusSW/as-http#readme",
  "dependencies": {
    "cross-fetch": "^3.0.6"
  },
  "devDependencies": {
    "@assemblyscript/loader": "^0.18.24",
    "as-console": "^4.0.3",
    "assemblyscript": "^0.18.24",
    "fastify": "^3.14.2"
  }
}