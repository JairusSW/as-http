{
  "name": "as-http",
  "version": "1.0.0",
  "description": "Http Client For AssemblyScript",
  "main": "imports.js",
  "scripts": {
    "test": "node tests",
    "asbuild:untouched": "asc assembly/HTTP.ts --exportTable --exportRuntime --target debug",
